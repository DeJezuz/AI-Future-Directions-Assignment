def representative_dataset_gen():
    """Generator function to yield representative samples for quantization."""
    # Use a small subset of the training data (e.g., 100 samples)
    for i in range(100):
        # The TFLite converter expects a list of numpy arrays
        # Each array corresponds to one input tensor of the model
        yield [train_images[i:i+1]]
